{
    "agent": "DQN_SWR_frozen_1M",
    "problem": "Foragax",
    "total_steps": 10000000,
    "episode_cutoff": -1,
    "metaParameters": {
        "epsilon": 0.05,
        "target_refresh": 1,
        "buffer_size": 1000,
        "buffer_min_size": 50,
        "batch": 32,
        "environment": {
            "env_id": "ForagaxDiwali-v1",
            "aperture_size": 5,
            "observation_type": "rgb"
        },
        "experiment": {
            "seed_offset": 0,
            "video_length": 1000
        },
        "gamma": 0.99,
        "optimizer": {
            "name": "Adam",
            "alpha": 0.001,
            "beta1": 0.9,
            "beta2": 0.9,
            "eps": 1e-08
        },
        "representation": {
            "type": "Forager2Net",
            "hidden": 64
        },
        "update_freq": 4,
        "swr": {
            "utility_function": "gradient",
            "pruning_method": "threshold",
            "reinit_freq": 2048,
            "reinit_factor": 1e-05,
            "decay_rate": 0.0
        },
        "freeze_steps": 1000000
    }
}