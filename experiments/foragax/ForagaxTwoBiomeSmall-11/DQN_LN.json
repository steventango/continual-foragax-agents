        "initial_epsilon": 1,
        "final_epsilon": 0.05,
        "epsilon_linear_decay": 0.8,
        "target_refresh": 1,
        "buffer_size": 10000,
        "buffer_min_size": "batch_size",
        "batch": 32,
        "environment": {
            "env_id": "ForagaxTwoBiomeSmall",
            "aperture_size": 11
        },
        "experiment": {
            "seed_offset": 0
        },
        "gamma": 0.99,
        "optimizer": {
            "name": "Adam",
            "alpha": 0.001,
            "beta1": 0.9,
            "beta2": 0.9,
            "eps": 1e-08
        },
        "representation": {
            "type": "Forager2LayerNormNet",
            "hidden": 64
        },
        "update_freq": 4
    }
